@model IEnumerable<HtmlHelperDemo.Models.User>
@{
    ViewBag.Title = "UserDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>UserDetails</h2>
<table>
    <tr>
        <th>UserId</th>
        <th>UserName</th>
        <th>Password</th>
        <th>Email</th>
    </tr>
    @foreach (var data in Model)
    {
    <tr>
        <td>@data.userid</td>
        <td>@data.username</td>
        <td>@data.password</td>
        <td>@data.email</td>
    </tr>
    }
</table>
